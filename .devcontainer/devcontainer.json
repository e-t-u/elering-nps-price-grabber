{
	"name": "Codespaces Rust Starter",
	"extensions": [
		"rust-lang.rust-analyzer",
		"serayuzgur.crates",
		"vadimcn.vscode-lldb",
		"tamasfe.even-better-toml",
		"github.copilot",
		"github.copilot-chat"
	],
	"dockerFile": "Dockerfile",
	"settings": {
		"editor.formatOnSave": true,
		"terminal.integrated.shell.linux": "/usr/bin/fish",
		"files.exclude": {
			"**/CODE_OF_CONDUCT.md": true,
			"**/LICENSE": true
		}
	}
}